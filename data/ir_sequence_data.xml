<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for Installments Payment -->
        <record id="seq_installments_payment" model="ir.sequence">
            <field name="name">Installments Payment Document Sequence</field>
            <field name="code">installments.payment</field>
            <field name="prefix">INPY/NMS/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>
